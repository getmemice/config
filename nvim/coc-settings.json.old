{
  "html-css-support.styleSheets": [],
  "html-css-support.enabledLanguages": [
    "html",
    "typescriptreact",
    "javascriptreact"
  ],
  "tailwindCSS.enable": true,
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.validate": true,
  "tailwindCSS.custom.serverPath": "~/.nvm/versions/node/v18.16.0/bin/tailwindcss-language-server",
  "tailwindCSS.includeLanguages": {
    "eelixir": "html",
    "elixir": "html",
    "eruby": "html",
    "html.twig": "html",
    "javascript": "html",
    "javascriptreact": "html",
    "typescript": "html",
    "typescriptreact": "html"
  },
  "bootstrap-classname.filetypes": [
    "html",
    "javascriptreact",
    "typescriptreact"
  ],
  "suggest.labelMaxLength": 60,
  "suggest.noselect": false,
  "suggest.enablePreselect": false,
  "suggest.triggerAfterInsertEnter": true,
  "suggest.autoTrigger": "always",
  "suggest.timeout": 5000,
  "suggest.detailField": "preview",
  "suggest.snippetIndicator": "",
  "suggest.triggerCompletionWait": 50,
  "suggest.completionItemKindLabels": {
    "variable": "💨",
    "constant": "",
    "struct": "פּ",
    "class": "",
    "interface": "",
    "text": "",
    "color": "",
    "enum": "",
    "enumMember": "",
    "property": "",
    "field": "4",
    "unit": "",
    "file": "",
    "value": "",
    "event": "鬒",
    "folder": "",
    "function": "ƒ",
    "keyword": "",
    "operator": "洛",
    "snippet": "",
    "reference": "渚",
    "typeParameter": "",
    "method": "",
    "module": "",
    "default": ""
  },
  "diagnostic.floatConfig": {
    "rounded": true,
    "border": true
  },
  "diagnostic.virtualTextFormat": "%message [%source]",
  "diagnostic.enableMessage": "always",
  "diagnostic.enableSign": true,
  "diagnostic.errorSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.displayByAle": false,
  "diagnostic.refreshOnInsertMode": false,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.virtualTextPrefix": " ❯❯❯ ",
  "diagnostic.virtualText": false,
  "yank.enableCompletion": false,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact"
  },
  "prettier.bracketSpacing": true,
  "prettier.enable": true,
  "prettier.semi": false,
  "snippets.ultisnips.enable": false,
  "coc.preferences.formatOnType": true,
  "coc.preferences.formatOnSave": true,
  "snippets.extends": {
    "javascriptreact": [
      "javascript"
    ],
    "typescript": [
      "javascript"
    ]
  },
  "cSpell.ignorePaths": [
    "**/package-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/objects/**",
    ".vscode"
  ],
  "cSpell.minWordLength": 4,
  "cSpell.numSuggestions": 8,
  "cSpell.allowCompoundWords": true,
  "cSpell.ignoreWords": [
    "",
    "airbnc",
    "alvan",
    "busa",
    "christoomey",
    "cobalt",
    "css",
    "dsznajder",
    "gertjanreynaert",
    "gruvbox",
    "haya",
    "honza",
    "junegunn",
    "justinmk",
    "linenr",
    "morhetz",
    "neoclide",
    "prettier",
    "ryanoasis",
    "scrooloose",
    "semi",
    "svermeulen",
    "tpope",
    "ultisnips",
    "vim",
    "yggdroot"
  ]
}
